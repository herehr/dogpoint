{
  "name": "dogpoint-backend",
  "version": "1.0.0",
  "private": true,
  "type": "commonjs",
  "engines": {
    "node": ">=20.12.2 <21"
  },
  "scripts": {
    "dev": "ts-node-dev src/index.ts",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit",
    "build": "npm run clean && tsc",
    "start": "node dist/index.js",
    "start:with-migrate": "npm run migrate:deploy && node dist/index.js",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:dev": "prisma migrate dev",
    "db:push": "prisma db push",
    "reset-admin": "node scripts/resetAdmin.mjs",
    "db:seed": "prisma db seed",
    "prisma:generate": "prisma generate --schema=./prisma/schema.prisma"
  },
  "dependencies": {
  "@aws-sdk/client-s3": "^3.882.0",
  "@prisma/client": "^6.16.2",
  "bcryptjs": "^2.4.3",
  "cors": "^2.8.5",
  "dotenv": "^16.4.5",
  "express": "^4.19.2",
  "jsonwebtoken": "^9.0.2",
  "mime-types": "^2.1.35",
  "multer": "^2.0.2",
  "stripe": "^18.5.0",

  "@types/bcryptjs": "^2.4.6",
  "@types/cors": "^2.8.19",
  "@types/express": "^4.17.23",
  "@types/jsonwebtoken": "^9.0.6",
  "@types/mime-types": "^2.1.4",
  "@types/multer": "^2.0.0"
},
"devDependencies": {
  "cross-env": "^10.0.0",
  "prisma": "^6.16.2",
  "rimraf": "^6.0.1",
  "ts-node": "^10.9.2",
  "ts-node-dev": "^2.0.0",
  "typescript": "^5.5.4"
}
}