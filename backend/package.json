{
  "name": "dogpoint-backend",
  "version": "1.0.0",
  "private": true,
  "type": "commonjs",
  "engines": {
    "node": ">=18 <21"
  },
  "scripts": {
    "dev": "ts-node-dev src/index.ts",
    "build": "prisma generate && tsc",
    "start": "node dist/index.js",
    "postinstall": "prisma generate",
    "migrate:deploy": "prisma migrate deploy",
